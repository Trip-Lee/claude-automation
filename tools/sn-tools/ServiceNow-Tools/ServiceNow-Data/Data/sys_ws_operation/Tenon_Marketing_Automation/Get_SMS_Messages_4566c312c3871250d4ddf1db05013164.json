  "operation_script": {
    "value": "(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\n\n\ttry{\n\t\tconst apiUtil = new x_cadso_automate.APIUtils();\n\t\tconst table = 'x_cadso_automate_message';\n\t\tconst query = request.queryParams.encoded_query;\n\t\tconst message = apiUtil.query({ table, query });\n\t\tif(!message){\n\t\t\treturn new sn_ws_err.NotFoundError('Could not find emails');\n\t\t}\n\t\tresponse.setBody(message);\n\t}catch(error){\n\t\treturn new sn_ws_err.BadRequestError(error.message);\n\t}\n\n\n})(request, response);",
