  "operation_script": {
    "value": "(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\r\n    try {\r\n        const skAPI = new x_cadso_automate.StyleKitAPI();\r\n        // const query = request.queryParams.encoded_query;\r\n        const response_body = skAPI.getStyleKits();\r\n\r\n        if (!response_body) {\r\n            return new sn_ws_err.NotFoundError('Could not find style kits');\r\n        }\r\n\r\n        response.setBody(response_body);\r\n    } catch (error) {\r\n        return new sn_ws_err.BadRequestError(error.message);\r\n    }\r\n})(request, response);",
