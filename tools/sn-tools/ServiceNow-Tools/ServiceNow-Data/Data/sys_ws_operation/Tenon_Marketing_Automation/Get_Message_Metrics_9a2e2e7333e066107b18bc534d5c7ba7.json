  "operation_script": {
    "value": "(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\r\n    try {\r\n        const {\r\n            messageID,\r\n            metric\r\n        } = request.pathParams;\r\n        //const event = request.queryParams.event[0];\r\n        const MESSAGE = new x_cadso_automate.MessageMS();\r\n        return MESSAGE[metric]({\r\n            sysID: messageID,\r\n            messageTable: 'message'\r\n        });\r\n    } catch (error) {\r\n        return new sn_ws_err.BadRequestError(error.message);\r\n    }\r\n})(request, response);",
