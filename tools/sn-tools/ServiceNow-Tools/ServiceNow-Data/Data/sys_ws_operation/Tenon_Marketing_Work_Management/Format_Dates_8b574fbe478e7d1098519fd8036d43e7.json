  "operation_script": {
    "value": "(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\n\n\tconst { dates } = request.body.data;\n\tconst scheduleWrapperApi = new x_cadso_work.ScheduleWrapper({});\n\tconst dateTimeFormat = scheduleWrapperApi.getUserFormat();\n\n\tconst tenonTimeApi = new x_cadso_work.TenonTime(true);\n\n\tObject.keys(dates).forEach(function(value) {\n\t\tconst displayValue = dates[value];\n\t\tconst formatedDisplayValue = tenonTimeApi.format(displayValue);\n\t\tdates[value] = formatedDisplayValue;\n\t});\n\n\tresponse.setBody({ dates, dateTimeFormat });\n\treturn response;\n\n})(request, response);",
    "display_value": "(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\n\n\tconst { dates } = request.body.data;\n\tconst scheduleWrapperApi = new x_cadso_work.ScheduleWrapper({});\n\tconst dateTimeFormat = scheduleWrapperApi.getUserFormat();\n\n\tconst tenonTimeApi = new x_cadso_work.TenonTime(true);\n\n\tObject.keys(dates).forEach(function(value) {\n\t\tconst displayValue = dates[value];\n\t\tconst formatedDisplayValue = tenonTimeApi.format(displayValue);\n\t\tdates[value] = formatedDisplayValue;\n\t});\n\n\tresponse.setBody({ dates, dateTimeFormat });\n\treturn response;\n\n})(request, response);"
