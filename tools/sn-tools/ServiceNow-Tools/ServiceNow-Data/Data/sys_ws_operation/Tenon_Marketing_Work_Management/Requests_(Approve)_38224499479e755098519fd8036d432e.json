  "operation_script": {
    "value": "(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\r\n    var { sysId } = request.body.data;\r\n\t\r\n    var approvalGR = new GlideRecordSecure('sysapproval_approver');\r\n\r\n    if (approvalGR.get(sysId)) {\r\n\t\tapprovalGR.setValue('state', 'approved');\r\n        approvalGR.update();\r\n\r\n        response.setStatus(200);\r\n        response.setBody({ message: 'Record state updated successfully.' });\r\n    } else {\r\n        response.setStatus(404);\r\n        response.setBody({ error: 'Record not found.' });\r\n    }\r\n})(request, response);",
