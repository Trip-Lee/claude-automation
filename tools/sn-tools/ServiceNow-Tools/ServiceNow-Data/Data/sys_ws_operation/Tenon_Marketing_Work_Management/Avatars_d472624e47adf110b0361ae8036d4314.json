  "operation_script": {
    "value": "(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\r\n\r\n\tvar { users = [] } = request.body.data;\r\n\t\t\t\t\r\n\tif (users.length == \"\" || !users.length){\r\n\t\treturn response.setBody({avatars: {}, 'found': false});\r\n\t}\r\n\t\r\n\tvar userGR = new GlideRecord('sys_user');\r\n\tuserGR.addQuery('sys_id', 'IN', users.join(','));\r\n\tuserGR.query();\r\n\t\r\n\tvar avatars = {};\r\n\tvar found = false;\r\n\t\r\n\twhile (userGR.next()) {\r\n\t\tavatars[userGR.getValue('sys_id')] = userGR.getValue('avatar') || userGR.getValue('photo');\r\n\t\tfound = true;\r\n\t}\r\n\t\r\n\tresponse.setBody({avatars, found});\r\n\r\n\treturn response;\r\n\t\r\n})(request, response);",
