  "operation_script": {
    "value": "(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\r\n\r\n    try {\r\n\t\tconst apiUtil = new x_cadso_automate.APIUtils();\r\n\t\tconst table = 'x_cadso_journey_version_step';\r\n\t\tconst data = request.body.data;\r\n\t\tdata.journey = request.pathParams.journeySysID;\r\n\t\tdata.version = request.pathParams.versionSysID;\r\n\t\tconst step = apiUtil.insert({ table, data });\r\n\t\treturn step;\r\n\t} catch(error) {\r\n\t\treturn new sn_ws_err.BadRequestError(error.message);\r\n\t}\r\n\r\n})(request, response);",
