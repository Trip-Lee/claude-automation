  "operation_script": {
    "value": "(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {\r\n\r\n    var journeyOptionGr = new GlideRecord('x_cadso_journey_option');\r\n    journeyOptionGr.addEncodedQuery('active=true');\r\n    journeyOptionGr.query();\r\n\tconst options = [];\r\n\r\n    while (journeyOptionGr.next()) {\r\n\t\tconst icon = journeyOptionGr.getValue('icon') + '';\r\n\t\toptions.push({\r\n\t\t\tdisplayValue: journeyOptionGr.getDisplayValue(),\r\n\t\t\tshortDescription: journeyOptionGr.getValue('short_description'),\r\n\t\t\tdescription: journeyOptionGr.getValue('description'),\r\n\t\t\ticon,\r\n\t\t\tisImage: icon.indexOf('.') > -1,\r\n\t\t\tvalue: journeyOptionGr.getUniqueValue()\r\n\t\t});\r\n    }\r\n\r\n\tresponse.setBody({ options });\r\n\treturn response;\r\n\r\n})(request, response);",
