  "script": {
    "value": "(function executeRule(current, previous /*null when async*/ ) {\r\n    const targetDayOfWeek = current.day_of_week.nil() ? 0 : Number(current.getValue(\"day_of_week\"));\r\n    const time = current.time.nil() ? \"\" : current.getValue(\"time\");\r\n    const jbDateTime = new JourneyBuilderDateTime();\r\n    const {\r\n        glideDateTime\r\n    } = jbDateTime.getNextDayOfWeekDateTime({\r\n        time,\r\n        dayOfTheWeek: targetDayOfWeek\r\n    });\r\n    current.setValue(\"datetime\", glideDateTime);\r\n})(current, previous);",
