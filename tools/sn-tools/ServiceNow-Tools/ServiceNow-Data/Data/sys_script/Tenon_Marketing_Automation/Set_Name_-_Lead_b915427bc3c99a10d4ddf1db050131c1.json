  "script": {
    "value": "(function executeRule(current, previous /*null when async*/) {\n\tconst leadGr = current.lead.getRefRecord();\n\tif (!leadGr.isValidRecord()) return;\n\tconst firstName = leadGr.getDisplayValue('first_name');\n\tconst lastName = leadGr.getDisplayValue('last_name');\n\tlet name = firstName ? firstName : '';\n\tif (lastName) {\n\t\tname = name ? name + ' ' : name;\n\t\tname += lastName;\n\t}\n\tcurrent.setValue('name', name);\n\n})(current, previous);",
