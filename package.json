{
  "name": "claude-automation",
  "version": "0.14.0",
  "description": "Claude Multi-Agent Coding System - Installable AI-powered development orchestration",
  "type": "module",
  "bin": {
    "dev-tools": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "install": "node install.js",
    "postinstall": "echo '\nRun \"node install.js\" to complete installation\n'",
    "test": "node test/run-unit-tests.js",
    "test:unit": "node test/run-unit-tests.js",
    "test:smoke": "node test/smoke-test.js",
    "test:validate": "node test/validation-suite.js",
    "test:all": "npm run test:unit && npm run test:smoke && npm run test:validate"
  },
  "keywords": [
    "ai",
    "automation",
    "claude",
    "docker",
    "orchestration",
    "multi-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@octokit/rest": "^21.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "date-fns": "^3.3.1",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1",
    "uuid": "^13.0.0",
    "yaml": "^2.4.0",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
